from pyrogram import filters
from pyrogram.types import CallbackQuery

from userge import userge, Config
from userge.utils import check_owner


@userge.on_callback_query(filters.regex("^Unblock$")
@check_owner
async def unblock_pm(_, c_q: CallbackQuery):
    await userge.send_message(Config.LOG_CHANNEL_ID, f"User {message.chat.title} was unblocked...")
